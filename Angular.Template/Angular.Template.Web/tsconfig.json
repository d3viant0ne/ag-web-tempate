{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "traceResolution": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "lib": [ "dom", "es2015" ],
    "typeRoots": [
      "node_modules/@types/"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
